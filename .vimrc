set nocompatible

filetype on

filetype plugin on

filetype indent on
set number

syntax on

set shiftwidth=4

set tabstop=4

set expandtab

set nobackup

set scrolloff=10

set nowrap

set incsearch

set ignorecase

set smartcase

set showcmd

set showmode

set showmatch

set hlsearch

set history=1000

colorscheme industry
